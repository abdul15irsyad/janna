{
  "name": "janna",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npx typeorm-ts-node-commonjs -d src/database/database.datasource.ts",
    "migrate:create": "npx typeorm-ts-node-commonjs migration:create",
    "migrate:drop": "yarn typeorm schema:drop",
    "migrate:generate": "yarn typeorm migration:generate",
    "migrate:run": "yarn typeorm migration:run",
    "migrate:revert": "yarn typeorm migration:revert",
    "migrate:refresh": "yarn migrate:drop && yarn migrate:run",
    "seeder": "npx ts-node node_modules/@jorgebodega/typeorm-seeding/dist/cli.js seed -d src/database/database.datasource.ts",
    "seed:run": "yarn seeder",
    "seed:all": "yarn seeder src/database/seeders/*.seeder.ts",
    "prepare": "husky install",
    "git:commit": "cz",
    "git:push": "git push -u origin HEAD",
    "typecheck": "tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@apollo/server": "^4.8.0",
    "@jorgebodega/typeorm-seeding": "^6.0.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "10.0.3",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "10.0.3",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "10.0.3",
    "@nestjs/platform-socket.io": "10.0.3",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "10.0.3",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "graphql": "^16.7.1",
    "handlebars": "^4.7.7",
    "ioredis": "^5.3.2",
    "nestjs-form-data": "^1.9.0",
    "nestjs-i18n": "^10.2.6",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.0",
    "rxjs": "^7.8.0",
    "slugify": "^1.6.5",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.7",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.10",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "^16.18.16",
    "@types/passport-jwt": "^3.0.9",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.7",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^13.2.3",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "^5.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz.config.js"
    }
  }
}
